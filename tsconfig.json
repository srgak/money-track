/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "paths": {
      "additional": [
        "dist/additional/additional",
        "dist/additional"
      ],
      "autocomplete": [
        "dist/autocomplete/autocomplete",
        "dist/autocomplete"
      ],
      "chart": [
        "dist/chart/chart",
        "dist/chart"
      ],
      "date-picker": [
        "dist/date-picker/date-picker",
        "dist/date-picker"
      ],
      "field": [
        "dist/field/field",
        "dist/field"
      ],
      "password": [
        "dist/password/password",
        "dist/password"
      ],
      "rise-input": [
        "dist/rise-input/rise-input",
        "dist/rise-input"
      ],
      "select": [
        "dist/select/select",
        "dist/select"
      ],
      "sms-code": [
        "dist/sms-code/sms-code",
        "dist/sms-code"
      ],
      "transliteration": [
        "dist/transliteration/transliteration",
        "dist/transliteration"
      ]
    },
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "module": "es2020",
    "lib": [
      "es2020",
      "dom"
    ],
    "types": ["node"]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true,
    "strictPropertyInitialization": false
  }
}
